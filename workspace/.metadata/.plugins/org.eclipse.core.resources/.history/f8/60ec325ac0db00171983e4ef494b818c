drop database if exists testdb4;
create dababase testdb4;
user testdb4;

create table users(
user_id primary key auto_increment,
user_naem varchar(255),
password varchar(255)
);

insert into users values(1,"taro","123");
insert into users values(2,"jiro","456");
insert into users values(3,"hanako","789");

create table inquiry(
name varchar(255),
qtype varchar(255),
body varchar(255)
);
